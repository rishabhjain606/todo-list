{
    "compilerOptions": {
        "module": "commonjs",
        "skipLibCheck": true,
        "target": "es6",
        "noImplicitAny": false,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": "."
    },
    "include": [
        "src/**/*",
        "*.ts"
    ],
    "exclude": [
        "node_modules/*"
    ]
}